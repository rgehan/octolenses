{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "jsx-boolean-value": false,
    "object-literal-sort-keys": false,
    "ban-types": false,
    "member-ordering": false,
    "max-classes-per-file": false,
    "jsx-no-lambda": false
  },
  "linterOptions": {
    "exclude": ["*.js", "scripts/*"]
  }
}
